{
  "name": "task-satisfier",
  "version": "0.4.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently npm:dev-watch npm:test-watch",
    "dev-watch": "nodemon --signal SIGINT --watch src --watch spec --exec \"concurrently npm:serve npm:prettify\"",
    "prettify": "prettier -l --write \"**/*.js\"",
    "serve": "node src/local.js",
    "test": "jest",
    "test-watch": "jest --watchAll"
  },
  "dependencies": {
    "aws-sdk": "^2.814.0",
    "pg": "^7.6.1",
    "tmp": "0.0.33"
  },
  "devDependencies": {
    "concurrently": "4.0.1",
    "jest": "24.9.0",
    "nodemon": "2.0.2",
    "prettier": "1.15.2"
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": true
  }
}
